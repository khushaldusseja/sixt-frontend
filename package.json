      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(d3-color|d3-interpolate|react-toastify|animate.css|react-leaflet|react-apexcharts|apexcharts)/)",
      "<rootDir>/src/actions/offerList.js",
      "<rootDir>/src/reducer/offerList",
      "<rootDir>/src/router/index",
      "<rootDir>/src/routes/routes.js",
      "<rootDir>/src/index",
      "<rootDir>/src/reportWebVitals"
    ],
    "collectCoverageFrom": [
      "!src/setupProxy.js",
      "!src/features/newCohort/CohortEdit/RulesList.tsx",
      "!src/hooks/axiosInterceptors.js"
